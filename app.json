{
  "expo": {
    "name": "visuEats",
    "slug": "visuEats",
    "version": "2.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.visueats.visuEats",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses your camera to scan QR codes for quick menu lookups."
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "2020 MVP",
    "android": {
      "package": "com.visueats.visuEats",
      "permissions": ["CAMERA"]
    }
  }
}
